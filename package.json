{
  "private": true,
  "name": "eln-graphql-server",
  "version": "1.0.0",
  "description": "GraphQL server for ELN data",
  "scripts": {
    "build": "npm run build-ts",
    "build-ts": "tsc",
    "start": "node dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/eln-graphql-server.git"
  },
  "bugs": {
    "url": "https://github.com/cheminfo/eln-graphql-server/issues"
  },
  "homepage": "https://github.com/cheminfo/eln-graphql-server#readme",
  "dependencies": {
    "rest-on-couch": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^10.12.3",
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.6",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.6"
  }
}
